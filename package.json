{
  "name": "lol-explorer-server",
  "version": "0.0.0",
  "private": true,
  "main": "src/main.js",
  "scripts": {
    "init:mongo": "ts-node ./script/init-mongo.ts",
    "start": "webpack --watch",
    "build:dev": "cross-env NODE_ENV=development webpack",
    "build:prod": "cross-env NODE_ENV=production webpack",
    "test": "cross-env NODE_ENV=test nyc mocha -r ts-node/register ./test/*.spec.ts --exit",
    "script:summoner": "ts-node ./src/scripts/summoners.ts",
    "script:statistics": "ts-node ./src/scripts/statistics.ts",
    "script:cleanup": "ts-node ./src/scripts/cleanup.ts",
    "init:statistics": "ts-node ./src/scripts/init-statistics.ts"

  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "src/*"
    ],
    "exclude": [
      "**/db/*",
      "**/*.d.ts",
      "dist/*",
      "**/*.spec.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true,
    "cache": true
  },
  "dependencies": {
    "@types/supertest": "^2.0.7",
    "axios": "^0.18.0",
    "babel-loader": "^8.0.6",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.0.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "lodash": "^4.17.11",
    "mongoose": "^5.5.6",
    "morgan": "~1.9.1",
    "redis": "^2.8.0",
    "response-time": "^2.3.2",
    "supertest": "^4.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-flow": "^7.0.0",
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.1.7",
    "@types/cookie-parser": "^1.4.1",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.16.1",
    "@types/http-errors": "^1.6.1",
    "@types/lodash": "^4.14.129",
    "@types/mocha": "^5.2.7",
    "@types/mongoose": "^5.5.1",
    "@types/morgan": "^1.7.35",
    "@types/redis": "^2.8.12",
    "@types/response-time": "^2.3.3",
    "@types/sinon": "^7.0.13",
    "@types/sinon-mongoose": "^1.3.9",
    "awesome-typescript-loader": "^5.2.1",
    "chai": "^4.2.0",
    "core-js": "3",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.2",
    "mocha": "^6.1.4",
    "nodemon": "^1.19.0",
    "nodemon-webpack-plugin": "^4.0.8",
    "nyc": "^14.1.1",
    "sinon": "^7.3.2",
    "sinon-mongoose": "^2.3.0",
    "source-map-support": "^0.5.12",
    "ts-mock-imports": "^1.2.3",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-microsoft-contrib": "^6.1.1",
    "typescript": "^3.4.5",
    "typescript-tslint-plugin": "^0.3.1",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-middleware": "^3.6.2",
    "webpack-dev-server": "^3.3.1",
    "webpack-node-externals": "^1.7.2"
  }
}
